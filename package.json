{
    "name": "@ryan-liu/vite-plugin-qiniu-cdn-upload",
    "version": "1.1.1",
    "description": "基于 vite 的打包文件上传到七牛云的插件",
    "author": "ryan-liu",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "license": "MIT",
    "files": [
        "dist"
    ],
    "keywords": [
        "vite qiniu plugin"
    ],
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/LuckyRyan-web/vite-plugin-qiniu-cdn-upload"
    },
    "devDependencies": {
        "@ryan-liu/eslint-config-jsx-config": "^1.0.2",
        "@types/lodash": "^4.14.192",
        "eslint": "^7.32.0",
        "husky": "^7.0.2",
        "lint-staged": "^11.1.2",
        "prettier": "^2.4.1",
        "tslib": "^2.3.1",
        "tsup": "^6.7.0",
        "typescript": "^4.4.3",
        "vite": "^4.2.1"
    },
    "scripts": {
        "dev": "npm run build -- --watch",
        "build": "tsup",
        "lint": "lint-staged"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{ts,js}": [
            "eslint --fix --quiet"
        ],
        "*.json": [
            "prettier --write"
        ]
    },
    "dependencies": {
        "lodash": "^4.17.21",
        "qiniu": "^7.8.0"
    }
}
